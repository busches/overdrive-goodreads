{
	"name": "Goodreads Integration for Bridges",
	"version": "0.0.0",
	"description": "",
	"manifest_version": 3,
	"minimum_chrome_version": "91",
	"host_permissions": [
		"https://bridges.overdrive.com/*",
		"https://www.goodreads.com/*"
	],
	"content_scripts": [
		{
			"run_at": "document_start",
			"matches": [
				"https://bridges.overdrive.com/*"
			],
			"js": [
				"content.bundle.js"
			]
		}
	],
	"background": {
		"service_worker": "background.bundle.js"
	},
	"content_security_policy": {
		"script-src": "self",
		"object-src": "none"
	}
}