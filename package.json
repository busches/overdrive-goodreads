{
    "name": "bridges-goodread",
    "displayName": "Goodreads Integration for Bridges",
    "version": "0.0.0",
    "description": "A basic Plasmo extension.",
    "author": "busches",
    "packageManager": "npm@8.19.2",
    "scripts": {
        "dev": "plasmo dev",
        "build": "plasmo build",
        "lint:check": "prettier --check .",
        "lint:fix": "prettier --write .",
        "test": "jest"
    },
    "dependencies": {
        "plasmo": "0.57.2",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@jest/globals": "29.2.2",
        "@jest/types": "29.2.1",
        "@plasmohq/prettier-plugin-sort-imports": "3.5.2",
        "@testing-library/react": "13.4.0",
        "@types/chrome": "0.0.197",
        "@types/node": "18.8.3",
        "@types/react": "18.0.21",
        "@types/react-dom": "18.0.6",
        "jest": "29.2.2",
        "jest-environment-jsdom": "29.2.2",
        "jest-webextension-mock": "3.7.22",
        "prettier": "2.7.1",
        "ts-jest": "29.0.3",
        "typescript": "4.8.4"
    },
    "manifest": {
        "minimum_chrome_version": "91",
        "host_permissions": [
            "https://bridges.overdrive.com/*",
            "https://www.goodreads.com/*"
        ]
    }
}
