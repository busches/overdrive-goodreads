{
  "name": "bridges-goodreads",
  "version": "0.0.1",
  "description": "Goodreads Integration for Bridges",
  "scripts": {
    "build": "node utils/build.js",
    "lint": "xo",
    "lint-fix": "xo --fix"
  },
  "dependencies": {
    "dom-chef": "^5.0.0",
    "mem": "^8.1.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/core": "7.28.0",
    "@babel/preset-react": "7.27.1",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "9.0.0",
    "error-logger-webpack-plugin": "^1.1.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "source-map-loader": "^3.0.2",
    "webpack": "5.101.0",
    "webpack-cli": "4.7.0",
    "xo": "^0.40.1"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "globals": [
      "chrome"
    ],
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^React$"
        }
      ]
    },
    "ignore": [
      "utils/",
      "webpack.config.js"
    ]
  }
}
